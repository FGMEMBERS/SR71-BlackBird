<PropertyList>

	<path>Models/radar-base-dummy.ac</path>
	<texture-path>Models/Textures</texture-path>




    <animation include="Instrument-Light3.xml">
            <object-name>Radar-range-ComBde</object-name>
    </animation>

    <animation>
            <type>rotate</type>
            <object-name>Radar-range-Com</object-name>
            <property>/instrumentation/radar/range</property>
            <interpolation>
                    <entry>
                            <ind>10</ind>
                            <dep>0</dep>
                    </entry>
                    <entry>
                            <ind>20</ind>
                            <dep>70</dep>
                    </entry>
                    <entry>
                            <ind>40</ind>
                            <dep>140</dep>
                    </entry>
            </interpolation>
            <center>
                    <x-m>0</x-m>
                    <y-m>-0.0639756</y-m>
                    <z-m>-0.0636857</z-m>
            </center>
            <axis>
                    <x>-1</x>
                    <y>0</y>
                    <z>0</z>
            </axis>
    </animation>

    <animation>
            <type>select</type>
            <object-name>Temoin-RefCt-Actif</object-name>
            <condition>
                            <equals>
                                    <property>/systems/refuel/contact</property>
                                    <value>true</value>
                            </equals>
            </condition>
    </animation>


</PropertyList>