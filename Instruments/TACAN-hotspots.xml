<name>Panel Hotspots</name>
 <background>Aircraft/SR71-BlackBird/Panels/Textures/transparent-bg.rgb</background>
 <w>512</w>
 <h>512</h>
 <y-offset>0</y-offset>


 <instruments>
	<instrument>
		<name>Control</name>
		<x>256</x>
		<y>256</y>
		<w>512</w>
		<h>512</h>
		<w-base>1024</w-base>
		<h-base>1024</h-base>

		<actions>




<!--******************************************Tacan*************************************************-->
			<action>
				<name>Tacan-Channel-3</name>
				<button>0</button>
				<x>-200</x>
				<y>-150</y>
				<w>150</w>
				<h>200</h>
				<binding>
					<command>property-adjust</command>
					<property>/instrumentation/tacan/frequencies/valueCh3</property>
					<step>1</step>
					<min>0</min>
					<max>10</max>
					<wrap>true</wrap>
				</binding>
			</action>

			<action>
				<name>Tacan-Channel-2</name>
				<button>0</button>
				<x>-400</x>
				<y>-150</y>
				<w>150</w>
				<h>200</h>
				<binding>
					<command>property-adjust</command>
					<property>/instrumentation/tacan/frequencies/valueCh2</property>
					<step>1</step>
					<min>0</min>
					<max>10</max>
					<resolution>1</resolution>
					<wrap>true</wrap>
				</binding>
			</action>

			<action>
				<name>Tacan-Channel-1</name>
				<button>1</button>
				<x>-400</x>
				<y>-150</y>
				<w>150</w>
				<h>200</h>
				<binding>
					<command>property-adjust</command>
					<property>/instrumentation/tacan/frequencies/valueCh1</property>
					<step>1</step>
					<min>0</min>
					<max>2</max>
					<wrap>true</wrap>
				</binding>
			</action>

			<action>
				<name>Tacan-Channel-XY</name>
				<button>0</button>
				<x>-450</x>
				<y>250</y>
				<w>150</w>
				<h>150</h>
				<binding>
					<command>nasal</command>
					<script>nav.basculeXY();</script>
				</binding>
			</action>







