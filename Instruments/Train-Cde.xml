<PropertyList>

 <path>Models/train-cde.ac</path>
 <texture-path>Models/Textures</texture-path>



    <animation>
            <type>rotate</type>
            <object-name>Train-Cde</object-name>
            <property>/gear/gear/position-norm</property>
            <interpolation>
                    <entry>
                            <ind>0</ind>
                            <dep>-35</dep>
                    </entry>
                    <entry>
                            <ind>0.5</ind>
                            <dep>0</dep>
                    </entry>
                    <entry>
                            <ind>1</ind>
                            <dep>35</dep>
                    </entry>
            </interpolation>
            <center>
                <x-m>-0.06</x-m>
                <y-m>0</y-m>
                <z-m>0</z-m>
            </center>
            <axis>
                    <x>0</x>
                    <y>1</y>
                    <z>0</z>
            </axis>
    </animation>



</PropertyList>
