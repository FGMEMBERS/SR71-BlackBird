					<step>0.01</step>
					<min>26.0</min>
					<max>33.0</max>
					<wrap>false</wrap>
				</binding>
			</action>

			<action>
				<name>calibrate coarse</name>
				<button>1</button>
				<x>-370</x>
				<y>-500</y>
				<w>100</w>
				<h>180</h>
				<binding>
					<command>property-adjust</command>
