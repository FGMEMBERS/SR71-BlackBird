<!--*************************************altimeter ********************************-->
			<action>
				<name>calibrate fine</name>
				<button>0</button>
				<x>-490</x>
				<y>-500</y>
				<w>100</w>
				<h>180</h>
				<binding>
					<command>property-adjust</command>
					<property>/instrumentation/altimeter/setting-inhg</property>
					<step>-0.01</step>
					<min>26.0</min>
					<max>33.0</max>
					<wrap>false</wrap>
				</binding>
			</action>

			<action>
				<name>calibrate coarse</name>
				<button>1</button>
				<x>-490</x>
