                <h-base>1024</h-base>




                <actions>









    <!-- ************************************Instrument Light ********************************-->

                        <action>
                                <name>light-up</name>
                                <button>0</button>
                                <x>-200</x>
                                <y>-400</y>
                                <w>100</w>
                                <h>300</h>
                                <binding>
                                        <command>property-adjust</command>
                                        <property>/fdm/jsbsim/fcs/lighting-instruments-norm</property>
                                        <step>0.05</step>
                                        <min>0</min>
                                        <max>1</max>
                                </binding>
                        </action>
                        <action>
                                <name>light-down</name>
                                <button>0</button>
                                <x>-350</x>
                                <y>-400</y>
                                <w>100</w>
                                <h>300</h>
