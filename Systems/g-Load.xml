<?xml version="1.0"?>


<system name="g-Load">
	<channel name="g Load">
            <summer name="constant one">
                <input>fcs/aileron-cmd-norm</input>
                <clipto>
                    <min>1.0000</min>
                    <max>1.0000</max>
                </clipto>
            </summer>

            <summer name="g load">
                <input>-accelerations/n-pilot-z-norm</input>
                <input>-fcs/constant-one</input>
            </summer>

            <pure_gain name="g load norm">
                <input>fcs/g-load</input>
                <gain>0.1000</gain>
            </pure_gain>

            <summer name="neg g load norm">
                <input>fcs/g-load-norm</input>
                <clipto>
                    <min>-1.0000</min>
                    <max>0.0000</max>
                </clipto>
            </summer>

            <summer name="pos g load norm">
                <input>fcs/g-load-norm</input>
                <clipto>
                    <min>0.0000</min>
                    <max>1.0000</max>
                </clipto>
            </summer>

            <summer name="abs g load norm">
                <input>fcs/pos-g-load-norm</input>
                <input>-fcs/neg-g-load-norm</input>
            </summer>

            <summer name="g blend scheduler">
                <input>fcs/abs-g-load-norm</input>
                <input>velocities/mach</input>
            </summer>

            <scheduled_gain name="g load blend">
                <input>fcs/g-load-norm</input>
                <table>
                    <independentVar>fcs/g-blend-scheduler</independentVar>
                    <tableData>
                        0.0000	0.0000
                        1.0000	1.0000
                    </tableData>
                </table>
            </scheduled_gain>

            <pure_gain name="alpha norm">
                <input>aero/alpha-deg</input>
                <gain>0.0200</gain>
            </pure_gain>

            <scheduled_gain name="scheduled alpha norm">
                <input>fcs/alpha-norm</input>
                <table>
                    <independentVar>fcs/g-blend-scheduler</independentVar>
                    <tableData>
                        0.0000	1.0000
                        1.2000	0.0000
                    </tableData>
                </table>
            </scheduled_gain>

            <pure_gain name="pitch rate gain">
                <input>velocities/q-aero-rad_sec</input>
                <gain>1</gain>
            </pure_gain>

            <summer name="pitch cmd error">
                <input>fcs/pitch-trim-sum</input>
                <input>fcs/g-load-norm</input>
                <input>fcs/scheduled-alpha-norm</input>
                <input>fcs/pitch-rate-gain</input>
            </summer>

            <scheduled_gain name="g load p">
                <input>fcs/pitch-cmd-error</input>
                <table>
                    <independentVar>velocities/mach</independentVar>
                    <tableData>
                        0.2000	0.5000
                        1.8000	0.1000
                    </tableData>
                </table>
            </scheduled_gain>

            <integrator name="g load i">
                <input>fcs/pitch-cmd-error</input>
                <c1>0.0050</c1>
            </integrator>

            <lead_lag_filter name="g load d">
                <input>fcs/pitch-cmd-error</input>
                <c1>0.0010</c1>
                <c2>0.0001</c2>
                <c4>1.0000</c4>
            </lead_lag_filter>

            <summer name="g load command">
                <input>fcs/g-load-p</input>
                <clipto>
                    <min>-1.0000</min>
                    <max>1.0000</max>
                </clipto>
            </summer>

            <scheduled_gain name="pitch cmd norm">
                <input>fcs/g-load-command</input>
                <table>
                    <independentVar>velocities/mach</independentVar>
                    <tableData>
                        0.2000	1.0000
                        1.8000	1.0000
                    </tableData>
                </table>
            </scheduled_gain>
    </channel>



</system>
